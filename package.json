{
  "name": "@gliff-ai/annotate",
  "version": "0.23.0",
  "description": "gliff.ai ANNOTATE - a user friendly browser interface for annotating multidimensional images for machine learning",
  "repository": "git://github.com/gliff-ai/annotate.git",
  "main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx vite build -c vite.config.lib.ts && tsc --emitDeclarationOnly --outDir dist",
    "watch": "npx vite build -c vite.config.lib.ts --watch",
    "format": "npx --no-install prettier --write src/*.ts*",
    "prettier": "npx --no-install prettier -c src/*.ts*",
    "lint": "npm run lint:ts && npm run prettier",
    "lint:ts": "npx --no-install eslint src/*.ts*",
    "serve": "vite",
    "build:example": "vite build",
    "test": "jest --coverage --passWithNoTests"
  },
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@percy/cli": "^1.0.0-beta.67",
    "@gliff-ai/style": "^0.8.6",
    "@types/jest": "^26.0.21",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.1",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/utif": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mui-unused-classes": "^1.0.3",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.0.3",
    "prettier": "==2.3.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.1",
    "typescript": "^4.2.5",
    "vite": "^2.5.3",
    "vite-aliases": "^0.8.6"
  },
  "peerDependencies": {
    "@gliff-ai/style": "^0.8.6",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "react": "^17.0.0",
    "react-dom": "^17.0.1"
  },
  "dependencies": {
    "@gliff-ai/slpf": "^1.1.2",
    "@gliff-ai/upload": "^0.3.2",
    "react-inlinesvg": "^2.3.0",
    "simplify-js": "^1.2.4",
    "utif": "^3.1.0"
  }
}
