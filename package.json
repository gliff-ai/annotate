{
  "name": "@gliff-ai/annotate",
  "version": "1.0.0",
  "description": "gliff.ai ANNOTATE - a user friendly browser interface for annotating multidimensional images for machine learning",
  "repository": "git://github.com/gliff-ai/annotate.git",
  "main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "typings": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx --no-install vite build -c vite.config.lib.ts && tsc --emitDeclarationOnly --outDir dist && tsc-alias -p tsconfig.json",
    "watch": "npx --no-install vite build -c vite.config.lib.ts --watch",
    "format": "npx --no-install prettier --write src",
    "prettier": "npx --no-install prettier -c src",
    "lint": "npm run lint:ts && npm run prettier",
    "lint:ts": "npx --no-install eslint src",
    "serve": "vite",
    "build:example": "vite build",
    "test": "jest --coverage --passWithNoTests",
    "test:visual": "npx percy snapshot snapshots.yml",
    "test:e2e": "percy exec -- jest -c jest.e2e.config.js --no-colors",
    "dev": "vite --host"
  },
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@babel/preset-env": "^7.15.8",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@gliff-ai/eslint-config": "^0.1.3",
    "@gliff-ai/jest-browserstack-automate": "^5.0.6",
    "@mui/icons-material": "^5.2.5",
    "@mui/lab": "^5.0.0-alpha.64",
    "@mui/material": "^5.2.8",
    "@mui/styles": "^5.2.3",
    "@percy/cli": "^1.0.0-beta.67",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/react": "==17.0.2",
    "@types/react-dom": "==17.0.2",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/utif": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.18.0",
    "esbuild": "^0.14.0",
    "eslint": "^7.25.0",
    "jest": "^27.0.3",
    "prettier": "==2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.0.7",
    "tsc-alias": "^1.3.9",
    "typescript": "^4.2.5",
    "vite": "^2.5.3",
    "vite-aliases": "^0.8.6",
    "vite-plugin-checker": "^0.3.4"
  },
  "peerDependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/lab": "^5.0.0-alpha.64",
    "@mui/material": "^5.2.8",
    "@mui/styles": "^5.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@gliff-ai/slpf": "^1.1.2",
    "@gliff-ai/style": "file:gliff-ai-style-4.0.0.tgz",
    "@gliff-ai/upload": "^1.0.1",
    "@percy/selenium-webdriver": "^1.0.0",
    "@types/selenium-webdriver": "^4.0.16",
    "@use-gesture/react": "^10.1.1",
    "chromedriver": "^96.0.0",
    "csstype": "^3.0.10",
    "detect-browser": "^5.2.1",
    "react-inlinesvg": "^2.3.0",
    "selenium-webdriver": "^4.0.0",
    "simplify-js": "^1.2.4",
    "utif": "^3.1.0"
  }
}
